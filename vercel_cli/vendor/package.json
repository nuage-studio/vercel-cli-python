{
  "bin": {
    "vc": "./dist/vc.js",
    "vercel": "./dist/vc.js"
  },
  "dependencies": {
    "@vercel/blob": "1.0.2",
    "@vercel/build-utils": "11.0.1",
    "@vercel/express": "0.0.6",
    "@vercel/fun": "1.1.6",
    "@vercel/go": "3.2.3",
    "@vercel/hono": "0.0.14",
    "@vercel/hydrogen": "1.2.3",
    "@vercel/next": "4.11.3",
    "@vercel/node": "5.3.13",
    "@vercel/python": "5.0.0",
    "@vercel/redwood": "2.3.4",
    "@vercel/remix-builder": "5.4.10",
    "@vercel/ruby": "2.2.1",
    "@vercel/static-build": "2.7.18",
    "chokidar": "4.0.0",
    "jose": "5.9.6"
  },
  "description": "The command-line interface for Vercel",
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://vercel.com",
  "license": "Apache-2.0",
  "name": "vercel",
  "preferGlobal": true,
  "repository": {
    "directory": "packages/cli",
    "type": "git",
    "url": "https://github.com/vercel/vercel.git"
  },
  "scripts": {
    "build": "node scripts/build.mjs",
    "coverage": "codecov",
    "dev": "echo \"'pnpm dev [command]' has been removed. Use 'pnpm vercel [command]' instead.\" && exit 1",
    "test": "jest --reporters=default --reporters=jest-junit --env node --verbose --bail",
    "test-dev": "pnpm test test/dev/",
    "test-e2e": "rimraf test/fixtures/integration && pnpm test test/integration-1.test.ts test/integration-2.test.ts test/integration-3.test.ts",
    "type-check": "tsc --noEmit",
    "vc": "pnpm vercel",
    "vercel": "ts-node ./src/index.ts",
    "vitest-run": "vitest --config ./vitest.config.mts",
    "vitest-unit": "jest test/unit/ --listTests"
  },
  "version": "46.0.2"
}
